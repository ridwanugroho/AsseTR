
@{
    ViewData["Title"] = "AllUser";
    Layout = "_AdminLayout";
    var users = ViewData["allUsers"] as List<AsseTS.Models.User>;
}


    <table class="table table-dark">
        <thead>
            <tr>
                <th scope="col">No</th>
                <th scope="col">Name</th>
                <th scope="col">Role</th>
                <th scope="col">Options</th>
            </tr>
        </thead>
        <tbody>
            @{var i = 1;}
            @foreach(var u in users)
            {
                <tr>
                    <th scope="row">@i</th>
                    <td>@u.FullName</td>
                    <td>@u.Role</td>
                    <td>update</td>
                </tr>
            }
        </tbody>
    </table>

